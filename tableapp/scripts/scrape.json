{
    "_id": "metacritic",
    "startUrl": [
        "https://www.metacritic.com/"
    ],
    "selectors": [
        {
            "id": "movies-all-time",
            "type": "SelectorLink",
            "parentSelectors": [
                "_root"
            ],
            "selector": "a[section='Movies of All Time']",
            "multiple": false,
            "delay": 0
        },
        {
            "id": "paging-2",
            "type": "SelectorLink",
            "parentSelectors": [
                "movies-all-time"
            ],
            "selector": "li:nth-of-type(2) a.page_num",
            "multiple": false,
            "delay": 0
        },
        {
            "id": "movie-link",
            "type": "SelectorLink",
            "parentSelectors": [
                "paging-2",
                "paging-3",
                "paging-1",
                "paging-4",
                "paging-5",
                "paging-6",
                "paging-7"
            ],
            "selector": "a.title",
            "multiple": true,
            "delay": 0
        },
        {
            "id": "movie-title",
